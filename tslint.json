{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "ordered-imports": false,
    "no-implicit-dependencies": [true, "dev"],
    "jsx-no-lambda": false
  },
  "rulesDirectory": []
}